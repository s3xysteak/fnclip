{
  "name": "fnclip",
  "type": "module",
  "version": "0.3.2",
  "packageManager": "pnpm@10.18.0",
  "description": "_description_",
  "author": "s3xysteak",
  "license": "MIT",
  "homepage": "https://github.com/s3xysteak/fnclip",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/s3xysteak/fnclip.git"
  },
  "bugs": "https://github.com/s3xysteak/fnclip/issues",
  "keywords": [
    "function",
    "utils"
  ],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "fnclip": "./dist/run.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsdown --watch src",
    "build": "tsdown",
    "build:types": "tsc -p tsconfig.build.json",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "release": "pnpm prepare && pnpm test:once && pnpm typecheck && bumpp",
    "test": "vitest",
    "test:once": "vitest run",
    "typecheck": "tsc --noEmit",
    "prepare": "esno scripts/update-funcs-meta.ts",
    "docs:dev": "vitepress dev src",
    "docs:build": "pnpm build:types && vitepress build src",
    "docs:preview": "vitepress preview src"
  },
  "dependencies": {
    "ansis": "^4.2.0",
    "cac": "^6.7.14",
    "consola": "^3.4.2",
    "defu": "^6.1.4",
    "empathic": "^2.0.0",
    "fs-extra": "^11.3.2",
    "local-pkg": "^1.1.2",
    "pathe": "^2.0.3",
    "unconfig": "^7.3.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.4.1",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.6.2",
    "bumpp": "^10.2.3",
    "eslint": "^9.37.0",
    "esno": "^4.8.0",
    "pkg-types": "^2.3.0",
    "publint": "^0.3.13",
    "tinyexec": "^1.0.1",
    "tinyglobby": "^0.2.15",
    "tsdown": "^0.15.6",
    "typescript": "^5.9.3",
    "unocss": "^66.5.2",
    "unplugin-auto-import": "^20.2.0",
    "vite": "^7.1.9",
    "vitepress": "^1.6.4",
    "vitest": "^3.2.4",
    "vue": "^3.5.22"
  }
}
