{
  "name": "fnclip",
  "type": "module",
  "version": "2.0.2",
  "packageManager": "pnpm@10.19.0",
  "description": "_description_",
  "author": "s3xysteak",
  "license": "MIT",
  "homepage": "https://s3xysteak.github.io/fnclip/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/s3xysteak/fnclip.git"
  },
  "bugs": "https://github.com/s3xysteak/fnclip/issues",
  "keywords": [
    "function",
    "utils"
  ],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "fnclip": "./dist/run.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsdown --watch src",
    "boot": "esno scripts/boot.ts",
    "build": "tsdown",
    "build:types": "esno scripts/build-types.ts",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "release": "pnpm boot && pnpm test:once && pnpm typecheck && bumpp",
    "test": "vitest",
    "test:once": "vitest run",
    "typecheck": "tsc --noEmit",
    "docs:dev": "vitepress dev src",
    "docs:build": "pnpm build:types && vitepress build src",
    "docs:preview": "vitepress preview src",
    "update-funcs-meta": "esno scripts/update-funcs-meta.ts",
    "update-doc-url": "esno scripts/update-doc-url.ts"
  },
  "dependencies": {
    "ansis": "^4.2.0",
    "cac": "^6.7.14",
    "consola": "^3.4.2",
    "defu": "^6.1.4",
    "empathic": "^2.0.0",
    "local-pkg": "^1.1.2",
    "pathe": "^2.0.3",
    "unconfig": "^7.3.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.1.0",
    "@types/node": "^24.9.1",
    "bumpp": "^10.3.1",
    "eslint": "^9.38.0",
    "esno": "^4.8.0",
    "pkg-types": "^2.3.0",
    "publint": "^0.3.15",
    "tinyexec": "^1.0.1",
    "tinyglobby": "^0.2.15",
    "tsdown": "^0.15.10",
    "typescript": "^5.9.3",
    "unocss": "^66.5.4",
    "unplugin-auto-import": "^20.2.0",
    "vite": "^7.1.12",
    "vitepress": "^1.6.4",
    "vitest": "^4.0.3",
    "vue": "^3.5.22"
  }
}
